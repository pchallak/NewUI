<ext4:fragment onBeforeRender="getWeightHeightRatioData^growthChartsMCM">
  <ext4:panel title="Weight/Height Ratio for Age" layout="fit" />
    <ext4:panel title="Birth through age 3" layout="fit" />
    <ext4:chart sessionName="whrtioChart" animate="true" height="380" width="600" >
      <ext4:axis type="Numeric" position="left"   title="Weight/Height (Kg/cm)" field="whrtio" grid="true" minimum="0" maximum="1" />
      <ext4:axis type="Time"    position="bottom" title="Date (months)"         field="Date"   grid="true" />
      <ext4:series type="line" axis="left" xField="Date" yField="Weight" />
    </ext4:chart>
  </ext4:panel>
</ext4:fragment>

